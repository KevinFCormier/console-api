    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^1.3.5",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "graphql-type-json": "^0.2.1",
