{
  "name": "hcm-ui-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/v2/server.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node  ./build/v2/server.js",
    "start:development:v1": "nodemon ./src/v1/server.js --exec babel-node",
    "start:production:v1": "cross-env NODE_ENV=production node  ./build/v1/server.js",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf build && mkdir build",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext ,.js .",
    "lint:fix": "eslint --ext .jsx,.js --fix .",
    "test": "cross-env NODE_ENV=development jest --forceExit"
  },
  "repository": {
    "type": "git",
